<form action="/payment/results">
  
  <div>
    <label for="first">APR</label>
    <input id="first" type="text" placeholder="E.g. 5.42" name="rate">
  </div>

  <div>
    <label for="second">Number of years</label>
    <input id="second" type="text" placeholder="How many years to repay?" name="years"
  </div>

  <div>
    <label for="third">Principal<label>
    <input id="third" type="text" placeholder="How much principal?" name="principal">
  </div>

  <button>Calculate monthly payment</button>

</form>
